<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.horrors.newhorror">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.horrors.newhorror.App"
        android:allowBackup="true"
        android:icon="@drawable/ghost"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ghost"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.horrors.newhorror.view.FriendDisplay"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.horrors.newhorror.view.AboutFilm"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.horrors.newhorror.view.EnterActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.horrors.newhorror.view.LoadFilmInsert"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.horrors.newhorror.MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>